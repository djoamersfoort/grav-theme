{
    "parserOptions": {
        "ecmaVersion": 5,
        "sourceType": "script"
    },
    "extends": "google",
    "env": {
        "browser": true,
        "jquery": true,
        "node": true
    },
    "globals": {
        "dragula": true,
        "GoogleMapsLoader": true
    },
    "rules": {
        "comma-dangle": ["error", "never"],
        "no-bitwise": 2,
        "no-tabs": 2,
        "curly": 2,
        "eqeqeq": 2,
        "no-eval": 2,
        "no-shadow": 2,
        "no-unused-vars": 2,
        "no-sequences": 2,
        "no-new": 2,
        "strict": [2, "function"],
        "indent": [2, 4, {"SwitchCase": 1}],
        "no-use-before-define": [2, {"functions": false}],
        "quote-props": [2, "as-needed"],
        "wrap-iife": [2, "outside"],
        "consistent-return": 2
    }
}
